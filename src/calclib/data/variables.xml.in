  </category>
  <category>
    <_title>Physical Constants</_title>
    <category>
      <_title>Universal Constants</_title>
      <variable>
        <_title>Characteristic Impedance of Vacuum</_title>
        <value>magnetic_constant*c</value>
	<_name index="1" reference="true">characteristic_impedance</_name>
        <_name index="2" abbreviation="true" suffix="true" avoid_input="true">Z_0</_name>
      </variable>
      <variable>
        <_title>Magnetic Constant (Permeability of Free Space)</_title>
        <value>4E-7*pi*N*A^(-2)</value>
	<_name index="1" reference="true">magnetic_constant</_name>
        <_name index="2" abbreviation="true" suffix="true" unicode="true" avoid_input="true">μ_0</_name>
      </variable>
      <variable>
        <_title>Electric Constant (Permittivity of Free Space)</_title>
        <value>1/(magnetic_constant*c^2)</value>
	<_name index="1" reference="true">electric_constant</_name>
        <_name index="2" abbreviation="true" suffix="true" unicode="true" avoid_input="true">ε_0</_name>
      </variable>
      <variable>
        <_title>Newtonian Constant of Gravitation</_title>
        <value approximate="true">6.6742E-11*m^3*kg^(-1)*s^(-2)</value>
	<_name index="1" reference="true">newtonian_constant</_name>
        <_name index="2" abbreviation="true">G</_name>
      </variable>
      <variable>
        <_title>Planck Constant</_title>
        <value approximate="true">6.626 0693E-34*J*s</value>
	<_name index="1" reference="true">planck</_name>
      </variable>
      <variable>
        <_title>Planck Constant over 2 pi</_title>
        <value>planck/(2*pi)</value>
	<_name index="1" reference="true">planck2pi</_name>
      </variable>
      <variable>
        <_title>Planck Length</_title>
        <value>planck2pi/(planck_mass*c)</value>
	<_name index="1" reference="true">planck_length</_name>
        <_name index="2" abbreviation="true" suffix="true" avoid_input="true">l_p</_name>
      </variable>
      <variable>
        <_title>Planck Mass</_title>
        <value>(planck+c/newtonian_constant)^(1/2)</value>
	<_name index="1" reference="true">planck_mass</_name>
        <_name index="2" abbreviation="true" suffix="true" avoid_input="true">m_p</_name>
      </variable>
      <variable>
        <_title>Planck Temperature</_title>
        <value>planck_mass*c^2/k</value>
	<_name index="1" reference="true">planck_temperature</_name>
        <_name index="2" abbreviation="true" suffix="true" avoid_input="true">T_p</_name>
      </variable>
      <variable>
        <_title>Planck Time</_title>
        <value>planck_length/c</value>
	<_name index="1" reference="true">planck_time</_name>
        <_name index="2" abbreviation="true" suffix="true" avoid_input="true">t_p</_name>
      </variable>
      <variable>
        <_title>Speed of Light in Vacuum</_title>
        <value>299792458*m*s^(-1)</value>
	<_name index="1" reference="true" abbreviation="true">c</_name>
        <_name index="2">speed_of_light</_name>
      </variable>
    </category>
