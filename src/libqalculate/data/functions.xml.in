        <expression>(\x-\y)/\z</expression>
        <argument type="free" index="1">
          <_title>Cost</_title>
        </argument>
        <argument type="free" index="2">
          <_title>Salvage value</_title>
        </argument>
        <argument type="free" index="3">
          <_title>Life</_title>
        </argument>
