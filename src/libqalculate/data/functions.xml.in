        <expression>(-\A{0}-\z*(1+\x*\B{0})*(((1+\x)^\y-1)/\x))/((1+\x)^\y)</expression>
        <argument type="free" index="1">
          <_title>Interest Rate</_title>
        </argument>
        <argument type="free" index="2">
          <_title>Number of Periods</_title>
